<h1 class="text-center">Regression Changes</h1>
<div class="container">
  <table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Version ID</th>
      <th>Item Type</th>
      <th>Item ID</th>
      <th>Diff</th>
    </tr>
  </thead>
  <tbody>
    <% @versions.each do |version| %>
      <tr >
        <td><%= version.id %></td>
        <td><%= version.item_type %></td>
        <td><%= version.item_id %></td>
        <td><%= link_to  "Diff", version_path(version.id)%></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>